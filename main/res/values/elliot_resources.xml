<!--THEME AND STYLE-->
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/m_color_dark_cyan</item>
        <item name="colorPrimaryDark">@color/m_color_sdark_cyan</item>
        <item name="colorAccent">@color/m_color_dark_cyan</item>
        <item name="android:textColor">@color/m_color_sdark_cyan</item>
    </style>


    <style name="AppThemeDark" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/m_color_dark_cyan</item>
        <item name="colorPrimaryDark">@color/m_color_sdark_cyan</item>
        <item name="colorAccent">@color/m_color_dark_cyan</item>
        <item name="android:textColor">@color/m_color_sdark_cyan</item>
    </style>


    <style name="ElliotToolbarStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:background">?attr/colorPrimary</item>
    </style>


    <style name="ElliotAppBarWhiteText" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorControlNormal">@color/m_color_white</item>
    </style>


    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


<!--To change the drawer button / up button color-->
    <style name="ElliotDrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@color/m_color_white</item>
    </style>


    <style name="ElliotColoredButton" parent="Widget.AppCompat.Button">
        <item name="colorButtonNormal">@color/m_color_dark_cyan</item>
        <item name="android:textColor">@color/m_color_white</item>
    </style>


    <style name="ElliotTitleTextColor">
        <item name="titleTextColor">@color/m_color_white</item>
    </style>


    <style name="SplashTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/logo</item>
    </style>

